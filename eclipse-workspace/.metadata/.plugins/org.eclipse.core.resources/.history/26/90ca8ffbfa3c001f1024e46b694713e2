package Day1_1;

public class CastingTest {

	public static void main(String[] args) {
		byte b = 100;
		int i = -2100000000;
		System.out.println(b);
		System.out.println(i);
		i = b;	// byte => int 자동 형 변환(Promotion)
		System.out.println(i);
		
		double d = b;	//byte => double 자동 형 변환(Promotion)
		System.out.println(d);
	}

}
